{
  "name": "assistant-os",
  "version": "0.2.0",
  "description": "Operating System and dependencies ready to assist human beings in automatizable tasks",
  "main": "src/assistant.js",
  "scripts": {
    "test": "mocha test/test-parser.js",
    "dev": "npx babel-node --config-file ./.babelrc.js scripts/start-all.js",
    "cli": "npx babel-node --config-file ./.babelrc.js nodes/adapters/command-line/index.js",
    "build": "NODE_ENV=production lerna exec -- babel *.js --ignore dist -d dist",
    "webpack": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-os/assistant-os.git"
  },
  "keywords": [
    "ai",
    "bot",
    "natural",
    "language"
  ],
  "author": "Thibault Friedrich <thibault.friedrich@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assistant-os/assistant-os/issues"
  },
  "workspaces": [
    "packages/config",
    "packages/utils",
    "packages/os",
    "packages/adapters/*",
    "packages/modules/*"
  ],
  "homepage": "https://github.com/assistant-os/assistant-os#readme",
  "private": true,
  "devDependencies": {
    "@assistant-os/config": "^1.0.0",
    "@assistant-os/os": "^1.0.0",
    "@assistant-os/utils": "^1.0.0",
    "@babel/node": "^7.2.2",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "lerna": "^3.13.1",
    "mocha": "^5.2.0",
    "chalk": "^2.4.2"
  }
}
