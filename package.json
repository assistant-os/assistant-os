{
  "name": "assistant-os",
  "version": "0.2.0",
  "description": "Operating System and dependencies ready to assist human beings in automatizable tasks",
  "main": "src/assistant.js",
  "scripts": {
    "test": "mocha test/test-parser.js",
    "dev": "npx babel-node --config-file ./.babelrc.js scripts/start-all.js",
    "cli": "npx babel-node --config-file ./.babelrc.js nodes/adapters/command-line/index.js",
    "build": "NODE_ENV=production lerna exec -- babel *.js --ignore dist -d dist",
    "webpack": "webpack --config webpack.config.js",
    "commitlint": "commitlint --from 04bf72d3d3d5e176b0df2d68adcd91370a14af92",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint --fix **/*.js",
    "format": "prettier --check '**/*.js'",
    "format:fix": "prettier --write '**/*.js'",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-os/assistant-os.git"
  },
  "keywords": [
    "ai",
    "bot",
    "natural",
    "language"
  ],
  "author": "Thibault Friedrich <thibault.friedrich@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assistant-os/assistant-os/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/assistant-os/assistant-os#readme",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "@assistant-os/config": "^0.3.2",
    "@assistant-os/os": "^0.3.2",
    "@assistant-os/hello": "^0.3.2",
    "@assistant-os/movies": "^0.3.2",
    "@assistant-os/prototype": "^0.3.2",
    "@assistant-os/utils": "^0.3.2",
    "chai": "^4.2.0",
    "dotenv": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^9.1.0",
    "husky": "^3.0.0",
    "lerna": "^3.13.1",
    "mocha": "^5.2.0",
    "prettier": "1.18.2",
    "chalk": "^2.4.2"
  }
}
