{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6
  },
  "rules": {
    "semi": [2, "never"],
    "arrow-body-style": 0,
    "no-const-assign": "error",
    "eqeqeq": "error",
    "curly": "error",
    "strict": "error",
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0
   },
   "extensions": [ ".js" ]
}
